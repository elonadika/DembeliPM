doctype html
html
  head
    meta(charset='UTF-8')    
    title #{title}   
    meta(name='description', content='#{description}')  
    meta(name='author', content='Dembeli')
    meta(name='viewport', content='width=device-width, initial-scale=1')
    //if IE
      meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
    
    link(href='assets/css/bootstrap.min.css', rel='stylesheet')    
    link(href='assets/css/font-awesome.min.css', rel='stylesheet')    
    link(href='assets/css/style.css', rel='stylesheet')
    //if lt IE 9
      script(src='assets/js/html5shiv.js')
  body    
    #preloader
      #loader
        .dot        
        .dot        
        .dot        
        .dot        
        .dot        
        .dot        
        .dot        
        .dot        
        .lading    
    #main-menu.navbar.navbar-default.navbar-fixed-top(role='navigation')
      .navbar-header        
        button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-collapse')
          span.sr-only Toggle navigation          
          i.fa.fa-bars
      nav.collapse.navbar-collapse        
        ul#headernavigation.nav.navbar-nav
          li.active
            a(href='#page-top') Home 
          li
            a(href='#about') About 
          li
            a(href='#techstack') Tech Stack
          li
            a(href='#contributors') Contributors                          
          li
            a(href='#subscribe') Subscribe

    block content                                 
    footer#footer-section
      p.copyright       
        a(href='https://github.com/Dembeli') &copy; 2015 Dembeli. MIT License.
        a(href='http://jeweltheme.com') &nbsp;Jewel Theme
    
    script(type='text/javascript', src='assets/js/jquery-2.1.0.min.js')    
    script(type='text/javascript', src='assets/js/modernizr-2.8.0.min.js')    
    script(type='text/javascript', src='assets/js/plugins.js')    
    script(type='text/javascript', src='assets/js/functions.js')    
    script(type='text/javascript', src='assets/js/jquery.parallax.js')

    script(src='assets/js/analytics.js')